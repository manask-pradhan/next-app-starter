node_modules/

# Ignore build output
.next/
out/

# Ignore coverage reports
coverage/

# Ignore TypeScript build output
dist/
build/

# Ignore environment variables
.env
.env.local
.env.development.local
.env.test.local
.env.production.local

# Ignore log files
*.log

# Ignore generated CSS files (Tailwind output if generated)
public/**/*.css

# Ignore lock files (no formatting needed)
package-lock.json
yarn.lock
pnpm-lock.yaml

# Ignore misc config and metadata files
*.config.js
*.config.ts
*.d.ts

# Ignore static assets
public/assets/
